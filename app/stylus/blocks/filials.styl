.filials
  &__map
    width 100%
    height 500px
    background-color #ebebeb
    margin-bottom 28px
    @media $bigDesktopMax
      height 370px
  &__items
    display flex
    flex-wrap wrap
    width calc(100% + 28px)
    margin 0 -14px
    @media $tPortraitMax
      width 100%
      margin 0
  &__item
    width calc(100%/3 - 28px)
    margin 0 14px
    min-height 179px
    @media $bigDesktopMax
      min-height 130px
    @media $tPortraitMax
      width 100%
      margin 0 0 3px
    &-top
      display flex
      flex-wrap wrap
      background-color #f1f7ff
    &-left
      width calc(100% - 84px)
      padding 35px 30px 25px 75px
      @media $bigDesktopMax
        width calc(100% - 64px)
        padding 26px 30px 12px 53px
    &-title
      position relative
      color $color
      font-size 16px
      font-weight 500
      line-height 21px
      min-height 42px
      margin-bottom 6px
      @media $bigDesktopMax
        font-size 13px
        line-height 18px
        min-height 35px
      &:before
        @extend .before
        left -44px
        top -6px
        width 23px
        height 31px
        background url('data:image/png;base64,') no-repeat center
        background-size contain
        @media $bigDesktopMax
          left -31px
          top -4px
          width 17px
          height 24px
    &-address
      display block
      color rgba($color, .86)
      font-size 13px
      line-height 19px
      margin-bottom 26px
      letter-spacing 0.02em
      @media $bigDesktopMax
        font-size 12px
        line-height 17px
        margin-bottom 8px
    &-data
      display flex
      flex-direction column
      color $color
      font-size 16px
      font-weight 500
      line-height 22px
      @media $bigDesktopMax
        font-size 14px
        line-height 20px
      a
        color $color
        align-self flex-start
        &[href^="tel"]
          transition opacity .4s
          &:hover
            opacity .7
    &-right
      display flex
      flex-direction column
      flex-shrink 0
      width 84px
      border-left 3px solid #fff
      @media $bigDesktopMax
        width 64px
    &-btn
      display block
      position relative
      width 100%
      height 50%
      border-bottom 3px solid #ffffff
      transition background .4s
      &:hover
        background-color $blueLight
      &:nth-child(2)
        border-bottom none
      &:before
        @extend .before
        top 50%
        transform translateY(-50%)
        background-repeat no-repeat
        background-position 50% 50%
        transition background .4s
        background-size contain
        z-index 1
      &--feedback
        &:before
          width 37px
          height 24px
          left 22px
          background-image url('data:image/png;base64,')
          @media $bigDesktopMax
            width 26px
            height 18px
            left 17px
        &:hover
          &:before
            background-image url('data:image/png;base64,')
      &--record
        &:before
          width 34px
          height 34px
          left 24px
          background-image url('data:image/png;base64,')
          @media $bigDesktopMax
            width 24px
            height 34px
            left 19px
        &:hover
          &:before
            background-image url('data:image/png;base64,')
  &__bottom
    margin-top 47px
    margin-bottom -8px
    @media $bigDesktopMax
      margin-top 26px
      margin-bottom -8px
    .btn
      width 100%
      max-width 354px
      margin 0 4px 8px
      @media $bigDesktopMax
        max-width 260px
  &__departments
    margin-top 4px
  &__department
    background-color #f1f7ff
    margin-bottom 4px
    &:last-child
      margin-bottom 0
    &-toggle
      display block
      position relative
      width 100%
      padding 29px 30px 30px 71px
      text-align left
      color $color
      font-size 14px
      font-weight 500
      line-height 20px
      letter-spacing 0.02em
      background none
      border none
      outline none
      user-select none
      transition color .4s
      span
        position absolute
        left 29px
        top 29px
        width 22px
        height 22px
        background-color #076af2
        cursor pointer
        &:before, &:after
          content ''
          display block
          position absolute
          left 6px
          top 10px
          width 10px
          height 2px
          background-color #fff
          transition transform .4s
        &:after
          transform rotate(90deg)
      &:hover, &.active
        color $blueLight
      &.active
        span
          &:after
            transform rotate(0)
    &-content
      display none
      position relative
      padding 0 30px 19px 71px
      top -9px
    &-shedule
      margin-bottom 18px
      margin-top -5px
      &-title
        color $color
        font-size 14px
        font-weight 500
        line-height 21px
        margin-bottom 8px
        letter-spacing 0.02em
      //&-list
      &-item
        display flex
        flex-wrap wrap
        margin-bottom 7px
      &-left, &-right
        font-size 13px
        font-weight 400
        line-height 17px
        letter-spacing .02em
        position relative
        color rgba(#1d1f20, .86)
        width calc(100% - 190px)
        z-index 1
        span
          display inline-block
          vertical-align top
          background #EFF7FF
          padding-right 10px
      &-left
        &:after
          content ''
          display block
          position absolute
          left 0
          bottom 6px
          width 100%
          height 1px
          border-bottom 1px dotted #1d1f20
          z-index -1
      &-right
        width 100%
        max-width 190px
        padding-left 13px
        color #1d1f20
    &-worker
      margin-bottom 35px
      &:last-child
        margin-bottom 0
      &-pos
        width 100%
        margin-bottom 12px
        color $color
        font-size 14px
        font-weight 500
        line-height 21px
        letter-spacing 0.02em
      &-row
        display flex
        flex-wrap wrap
        align-items center
      &-overimg
        width 110px
        margin-right 29px
        flex-shrink 0
      //&-img
      &-info
        display flex
        flex-direction column
        width calc(100% - 140px)
        a
          align-self flex-start
      &-name, &-phone, &-email
        font-size 14px
        line-height 20px
      &-name, &-phone
        color $color
        letter-spacing .015em
      &-name
        margin-bottom 10px
      &-phone
        margin-bottom 9px
        transition opacity .4s
        &:hover
          opacity .7
      &-email
        color $blueLight
        border-bottom 1px solid $blueLight
        transition border .4s
        &:hover
          border-bottom-color transparent
