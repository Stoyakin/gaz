.ymaps-2-1-74-map-copyrights-promo,
.ymaps-2-1-74-copyright
  display none !important

.ymaps-2-1-74-zoom
  padding 32px 0 !important
  width 32px !important
  border-radius 32px !important
  background-color #fff
  box-shadow 0 2px 6px 0 rgba(0,0,0,.2)
  overflow hidden
  &:after
    content ''
    position absolute
    height 2px
    top 50%
    transform: translateY(-50%)
    left 6px
    right 6px
    background-color #f5f5f5
    z-index 1
  &__icon
    border none !important
    width 30px !important
  &__plus
    .ymaps-2-1-74-zoom__icon
      height 32px !important
      background url('../../../../static/img/plus.svg') no-repeat center !important
  &__minus
    .ymaps-2-1-74-zoom__icon
      height 28px !important
      background url('../../../../static/img/minus.svg') no-repeat center !important
.ymaps-2-1-74-float-button
  height 32px !important
  padding-left 32px !important
  box-shadow none !important

.ymaps-2-1-74-controls__control
  @media $bigDesktopMax
    top 150px !important
