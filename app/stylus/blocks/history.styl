.history
  margin-bottom 142px
  @media $bigDesktopMax
    margin-bottom 80px
  &:last-child
    margin-bottom 0
  &__title
    margin-bottom 39px
    font-size 34px
    letter-spacing 0
    @media $bigDesktopMax
      font-size 24px
      margin-bottom 34px
  &__row
    display flex
    flex-wrap wrap
    align-items center
    justify-content space-between
    @media $bigDesktopMax
      align-items flex-start
  &__col
    width calc(50% - 15px)
    @media $tPortraitMax
      width 100%
    &:nth-child(2)
      padding-left 115px
      padding-top 11px
      @media $bigDesktopMax
        padding-top 0
        margin-top -6px
        padding-left 6%
        .wis-content p:not([class])
          margin 14px 0
      @media $tPortraitMax
        padding-left 0
        padding-top 40px
  &__gallery
    width calc(100% + 50px)
    margin-left -50px
    @media $tPortraitMax
      margin-left 0
      width 100%
    &-img
      display block
      width 100%
      padding-top 67.724%
      background-size cover
      background-position 50% 50%
      background-repeat no-repeat
  //&__text
  &__raiting
    position relative
    margin-top 44px
    max-width 249px
    background-color #076af2
    padding 17px 32px 20px
    overflow hidden
    z-index 1
    @media $bigDesktopMax
      margin-top 35px
      padding 17px 25px 20px
      max-width 237px
    &:after
      @extend .after
      width 96px
      height 91px
      top 6px
      right -49px
      background url('data:image/png;base64,') no-repeat center
      opacity .1
      z-index -1
    &:first-child
      margin-top 0
    &-title
      color #fff
      font-family GothamPro
      font-size 16px
      font-weight 500
      line-height 24px
      margin-bottom 10px
      @media $bigDesktopMax
        font-size 13px
        line-height 18px
        margin-bottom 4px
    &-source
      color rgba(#fff, .7)
      font-size 14px
      font-weight 400
      line-height 20px
      letter-spacing 0.015em
      a
        color rgba(#fff, .7)
        border-bottom 1px solid rgba(#fff, .7)
        transition border .4s
        &:hover
          border-bottom-color transparent
  .swiper
    &-ind
      display flex
      align-items center
      position absolute
      left 50%
      transform translateX(-50%)
      bottom 38px
      user-select none
      z-index 2
      @media $bigDesktopMax
        bottom 26px
      @media $minDesktopMax
        left 30px
        transform none
      @media $tPortraitMax
        display none
      &-number
        font-size 13px
        font-weight 500
        line-height 20px
        text-transform uppercase
        &--prev
          color #076af2
        &--next
          color $color
      &-line
        position relative
        background-color white
        width 444px
        height 2px
        flex-shrink 0
        margin 0 20px
        overflow hidden
        @media $bigDesktopMax
          width 332px
        span
          display block
          height 2px
          position absolute
          left 0
          top 0
          background-color $blueLight
    &-nav
      display flex
      align-items center
      background-color white
      position absolute
      bottom 0
      right 0
      z-index 2
      user-select none
      justify-content flex-end
      width 393px
      @media $bigDesktopMax
        width 306px
      @media $mPortraitMax
        width 235px
    &-count
      display flex
      align-items flex-start
      @media $bigDesktopMax
        margin-top 3px
      &-item
        color #076af2
        font-size 60px
        line-height 52px
        font-weight 400
        text-transform uppercase
        @media $bigDesktopMax
          font-size 44px
          line-height 36px
      &-all
        position relative
        color rgba(black, .2)
        font-size 18px
        line-height 22px
        font-weight 400
        text-transform uppercase
        margin-left 14px
        margin-top 2px
        min-width 26px
        @media $bigDesktopMax
          font-size 14px
          line-height 14px
        &:before
          content '/'
          display block
          position absolute
          left -11px
          top 2px
          color rgba(black, .2)
          font-size 18px
          line-height 18px
          font-weight 400
          text-transform uppercase
          @media $bigDesktopMax
            font-size 14px
            line-height 14px
