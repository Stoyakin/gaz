              li.page-list-item
                a(target="_blank" href="inner-services-insurance.html").link Услуги - страхование
              li.page-list-item
                a(target="_blank" href="inner-credit-calculator.html").link Услуги - кредит
              li.page-list-item
                a(target="_blank" href="inner-service.html").link Сервис
              li.page-list-item
