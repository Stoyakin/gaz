              li.page-list-item
                a(target="_blank" href="inner-action.html").link Акции
              li.page-list-item
                a(target="_blank" href="inner-action-item.html").link Страница акции
              li.page-list-item
                a(target="_blank" href="inner-map-site.html").link Карта сайта
              li.page-list-item
