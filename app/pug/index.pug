include includes/bemto/bemto.pug
include includes/__mixins

doctype html
html(lang="ru")
  - var title = "Разводящая"
  include includes/partials/head
  style(type='text/css').
    .page-list {
      display: inline-block;
      vertical-align: top;
      width: 48%;
      margin: 30px 0 0 !important;
    }
    .page-list-item {
      margin: 0 0 10px !important;
    }
    .page-list-item:before {
      display: none;
      content: none;
    }
    .page-list-item a {
      color: #1c1c1c;
      font-size: 16px;
      line-height: 24px;
      font-weight: 400;
      border-bottom: 1px solid #1c1c1c;
      transition: color 0.5s, border 0.5s;
      -webkit-transition: color 0.5s, border 0.5s;
    }
    .page-list-item a:hover {
      border-bottom-color: transparent;
    }
    h3 {
      margin-bottom: 20px; 
    }
    
  body
    .wrapper(style="overflow-x: hidden; padding-top: 80px;")
      main.main(style="background: none;")
        .container
          .text
            ul.page-list(style="display:inline-block;vertical-align:top;width:45%;margin-right:35px;")
              h3 Страницы
              li.page-list-item
                a(target="_blank" href="main.html").link Главная
              li.page-list-item
                a(target="_blank" href="inner-default.html").link Типовая текстовая
              li.page-list-item
                a(target="_blank" href="inner-news.html").link Новости
              li.page-list-item
                a(target="_blank" href="inner-news-item.html").link Новости - страница новости
              li.page-list-item
                a(target="_blank" href="inner-events.html").link Мероприятия
              li.page-list-item
                a(target="_blank" href="inner-events-item.html").link Мероприятия - страница мероприятия
              li.page-list-item
                a(target="_blank" href="inner-spare-parts.html").link Запчасти
              li.page-list-item
                a(target="_blank" href="inner-brand-page.html").link Запчасти - страница бренда
              li.page-list-item
                a(target="_blank" href="inner-catalog.html").link Каталог
              li.page-list-item
                a(target="_blank" href="inner-catalog-card.html").link Каталог - карточка товара
              li.page-list-item
                a(target="_blank" href="inner-contacts.html").link Контакты
              li.page-list-item
                a(target="_blank" href="inner-about.html").link О компании
              li.page-list-item
                a(target="_blank" href="inner-services.html").link Услуги
              li.page-list-item
